{
  "name": "shassi-frontend",
  "version": "1.7.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run ng -- serve --proxy-config proxy.conf.json --aot",
    "start:de": "npm run start -- --i18n-file=src/i18n/messages.de.xlf --locale=de --i18n-format=xlf",
    "start:sw": "npm run build && npm run start:liteserver",
    "start:liteserver": "lite-server -c liteserver_config.js",
    "dev-server": "json-server -w mock-backend.json --middlewares mock-backend.middleware.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "e2e-dev": "ng e2e --dev-server-target=",
    "start:ext": "npm run start -- --host 0.0.0.0 --disable-host-check",
    "get:domain": "lt --port 4200 --subdomain shassi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.8",
    "@angular/cdk": "8.2.1",
    "@angular/common": "8.2.8",
    "@angular/compiler": "8.2.8",
    "@angular/core": "8.2.8",
    "@angular/forms": "8.2.8",
    "@angular/material": "8.2.1",
    "@angular/platform-browser": "8.2.8",
    "@angular/platform-browser-dynamic": "8.2.8",
    "@angular/pwa": "^0.803.6",
    "@angular/router": "8.2.8",
    "@angular/service-worker": "8.2.8",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/entity": "^8.3.0",
    "@ngrx/schematics": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "core-js": "^3.2.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
    "@angular/cli": "8.3.6",
    "@angular/compiler-cli": "8.2.8",
    "@angular/language-service": "8.2.8",
    "@ngrx/store-devtools": "^8.3.0",
    "@types/jasmine": "^3.4.1",
    "@types/jasminewd2": "^2.0.7",
    "@types/node": "^12.7.8",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.15.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "3.5.3"
  }
}
